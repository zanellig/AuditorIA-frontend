  "components": {
    "schemas": {
      "AudioInput": {
        "properties": {
          "audio_file": {
            "type": "string",
            "title": "Audio File"
          }
        },
        "type": "object",
        "required": ["audio_file"],
        "title": "AudioInput"
      },
      "ChannelTranscriptionOutput": {
        "properties": {
          "segments": {
            "items": {
              "$ref": "#/components/schemas/SegmentOutput"
            },
            "type": "array",
            "title": "Segments"
          }
        },
        "type": "object",
        "required": ["segments"],
        "title": "ChannelTranscriptionOutput"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "SegmentAnalysis": {
        "properties": {
          "sentiment": {
            "type": "string",
            "title": "Sentiment"
          },
          "sentiment_probas": {
            "type": "object",
            "title": "Sentiment Probas"
          },
          "emotion": {
            "type": "string",
            "title": "Emotion"
          },
          "emotion_probas": {
            "type": "object",
            "title": "Emotion Probas"
          },
          "hate_speech": {
            "type": "string",
            "title": "Hate Speech"
          },
          "hate_speech_probas": {
            "type": "object",
            "title": "Hate Speech Probas"
          }
        },
        "type": "object",
        "required": [
          "sentiment",
          "sentiment_probas",
          "emotion",
          "emotion_probas",
          "hate_speech",
          "hate_speech_probas"
        ],
        "title": "SegmentAnalysis"
      },
      "SegmentOutput": {
        "properties": {
          "text": {
            "type": "string",
            "title": "Text"
          },
          "start": {
            "type": "number",
            "title": "Start"
          },
          "end": {
            "type": "number",
            "title": "End"
          },
          "channel": {
            "type": "string",
            "title": "Channel"
          },
          "analysis": {
            "allOf": [
              {
                "$ref": "#/components/schemas/SegmentAnalysis"
              }
            ]
          }
        },
        "type": "object",
        "required": ["text", "start", "end", "channel"],
        "title": "SegmentOutput"
      },
      "TextInput": {
        "properties": {
          "text": {
            "type": "string",
            "title": "Text"
          }
        },
        "type": "object",
        "required": ["text"],
        "title": "TextInput"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      }
    }
  }
