{
  "openapi": "3.1.0",
  "info": {
    "title": "FastAPI",
    "version": "0.1.0"
  },
  "paths": {
    "/process": {
      "post": {
        "summary": "Process",
        "operationId": "process_process_post",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/Body_process_process_post"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "anyOf": [
                    {
                      "$ref": "#/components/schemas/ChannelTranscriptionOutput"
                    },
                    {
                      "type": "object"
                    }
                  ],
                  "title": "Response Process Process Post"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Body_process_process_post": {
        "properties": {
          "task_type": {
            "type": "string",
            "title": "Task Type"
          },
          "language": {
            "type": "string",
            "title": "Language"
          },
          "file": {
            "type": "string",
            "format": "binary",
            "title": "File"
          },
          "text": {
            "type": "string",
            "title": "Text"
          },
          "save_to_db": {
            "type": "boolean",
            "title": "Save To Db",
            "default": false
          },
          "return_immediate": {
            "type": "boolean",
            "title": "Return Immediate",
            "default": false
          },
          "diarize": {
            "type": "boolean",
            "title": "Diarize",
            "default": false
          },
          "split_channels": {
            "type": "boolean",
            "title": "Split Channels",
            "default": false
          },
          "analyze_sentiment": {
            "type": "boolean",
            "title": "Analyze Sentiment",
            "default": false
          }
        },
        "type": "object",
        "required": ["task_type"],
        "title": "Body_process_process_post"
      },
      "ChannelTranscriptionOutput": {
        "properties": {
          "segments": {
            "items": {
              "$ref": "#/components/schemas/SegmentOutput"
            },
            "type": "array",
            "title": "Segments"
          }
        },
        "type": "object",
        "required": ["segments"],
        "title": "ChannelTranscriptionOutput"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "SegmentAnalysis": {
        "properties": {
          "sentiment": {
            "type": "string",
            "title": "Sentiment"
          },
          "sentiment_probas": {
            "type": "object",
            "title": "Sentiment Probas"
          },
          "emotion": {
            "type": "string",
            "title": "Emotion"
          },
          "emotion_probas": {
            "type": "object",
            "title": "Emotion Probas"
          },
          "hate_speech": {
            "type": "string",
            "title": "Hate Speech"
          },
          "hate_speech_probas": {
            "type": "object",
            "title": "Hate Speech Probas"
          }
        },
        "type": "object",
        "required": [
          "sentiment",
          "sentiment_probas",
          "emotion",
          "emotion_probas",
          "hate_speech",
          "hate_speech_probas"
        ],
        "title": "SegmentAnalysis"
      },
      "SegmentOutput": {
        "properties": {
          "text": {
            "type": "string",
            "title": "Text"
          },
          "start": {
            "type": "number",
            "title": "Start"
          },
          "end": {
            "type": "number",
            "title": "End"
          },
          "channel": {
            "type": "string",
            "title": "Channel"
          },
          "analysis": {
            "allOf": [
              {
                "$ref": "#/components/schemas/SegmentAnalysis"
              }
            ]
          }
        },
        "type": "object",
        "required": ["text", "start", "end", "channel"],
        "title": "SegmentOutput"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      }
    }
  }
}
